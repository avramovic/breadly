{
    "name": "avram/breadly",
    "description": "The missing backend for App Inventor",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.0",
        "ext-gd": "*",
        "avram/caffeinated-db-repository": "0.1.3",
        "avram/composer-caffeinated-module-installer": "^0.2",
        "caffeinated/modules": "4.3.2",
        "fideloper/proxy": "^4.0",
        "intervention/image": "^2.4",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "~1.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "rachidlaasri/laravel-installer": "^3.2",
        "ralouphie/mimey": "^1.0",
        "tcg/voyager": "^1.0",
        "tymon/jwt-auth": "1.0.0-rc.2"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "symfony/thanks": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Breadly\\Modules\\": "modules/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "installer-types": ["module"],
        "installer-paths": {
            "modules/{$name}/": ["type:module"]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan module:optimize",
            "@php artisan cache:clear",
            "@php artisan route:clear",
            "@php artisan config:clear",
            "@php artisan view:clear"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://larapack.io"
        }
    ]
}
